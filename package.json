  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^6.1.0",
    "hardhat": "^2.26.3"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^5.4.0",
    "@selfxyz/contracts": "^1.2.3",
    "dotenv": "^17.2.2",
    "ethers": "^6.15.0"
