{
  "name": "linked-dao-contracts",
  "version": "1.0.0",
  "description": "Blockchain talent matching platform with Self Protocol integration",
  "main": "index.js",
  "scripts": {
    "compile": "hardhat compile",
    "test": "hardhat test",
    "deploy": "hardhat run scripts/deploy.js",
    "deploy:alfajores": "hardhat run scripts/deploy.js --network celo-alfajores",
    "deploy:sepolia": "hardhat run scripts/deploy.js --network celo-sepolia",
    "node": "hardhat node",
    "subgraph:setup": "cd hypergraph && npm install",
    "subgraph:codegen": "cd hypergraph && npm run codegen",
    "subgraph:build": "cd hypergraph && npm run build",
    "subgraph:deploy": "cd hypergraph && npm run deploy",
    "subgraph:update-config": "cd hypergraph && node update-config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/krishnavbajoria02/Linked-Dao.git"
  },
  "keywords": [
    "blockchain",
    "talent",
    "verification",
    "dao"
  ],
